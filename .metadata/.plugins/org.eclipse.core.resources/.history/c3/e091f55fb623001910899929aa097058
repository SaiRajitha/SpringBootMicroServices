package com.project1.rest.webservices.restfulwebservices.customer;

import org.springframework.http.HttpStatus;
import org.springframework.web.bind.annotation.ResponseStatus;

// not found exception otherwise it will show internal server error
@ResponseStatus(HttpStatus.NOT_FOUND)
public class CustomerNotFoudException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public CustomerNotFoudException(String arg0) {
		super(arg0);
	}
	
}
